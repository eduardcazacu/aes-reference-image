/include/ "system-conf.dtsi"
/ { 
	usb_phy0: usb_phy@0 {
	    compatible = "usb-nop-xceiv";
	    #phy-cells = <0>;
		reg = <0xe0002000 0x1000>;
		view-port=<0x170>;
		drv-vbus;
	};	
};

 
&usb0 {
	compatible = "xlnx,zynq-usb-2.20.a", "chipidea,usb2";
	status = "okay";
	dr_mode = "host";
	usb-phy = <&usb_phy0>;
};